dec6 =>

  input := io.stdin.read_string.trim.split "\n" .map (.split " " .filter !="" .as_array) .as_array
  h := input.count
  w := input[0].count
  tasks := (0..(w-1)).map x->(input[h-1][x], array h-1 y->input[y][x].parse_i64.val)

  part1 =>
    tasks.map t->
           if   t.0="*" then t.1.product
                        else t.1.sum
         .sum

  part2 =>

  say "$part1:$part2"
