dec6_part1 is

  input =>
    LM : mutate is
    o := LM ! ()->io.stdin.reader LM ! ()->
      for
        r Sequence u8 := [], r++n
        n := (io.buffered LM .read_bytes 10000)
      while n.count > 0 else
        r
    _ : String is
      public redef utf8 Sequence u8 => o

  lines := input.split "\n"

  l1 := lines[0].split.drop 1
  l2 := lines[1].split.drop 1
  races := l1.zip l2 t,d->(race t.parse_i64.val d.parse_i64.val)
  race2 := (race ((lines[0].split ":")[1].replace " " "").parse_i64.val
                 ((lines[1].split ":")[1].replace " " "").parse_i64.val)
  race(t, d) is
    public redef as_string String => "Race {t}ms {d}mm"
    wins =>
      (i64 0 .. t).map x->(d x t)
                .filter x->(x>d)
                .count
    wins2 =>
       # 0 = l²-t*l+d
       #
       # l1/2 = (+t +/- sqrt(t²-4d)/2
       rt := t.as_f64
       rd := d.as_f64
       l1 := rt - (((rt*rt-4.0*rd)/2).square_root)
       l2 := rt + (((rt*rt-4.0*rd)/2).square_root)
       say "{wins2.this.l1} {wins2.this.l2}"
       i1 := wins2.this.l1.as_i64
       i2 := wins2.this.l2.as_i64
       say "{wins2.this.l1} {wins2.this.l2}"
       say "{wins2.this.i1} {wins2.this.i2}"
       i2-i1

  d(l,t) => l*(t-l)

  p1 := races.map (.wins)
             .fold i32.type.product

  p2 := race2.wins2

  say "part1 $p1 part2 $p2"
