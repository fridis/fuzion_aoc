dec25 =>

  input := io.stdin.read_string.split "\n\n" .map (.split "\n" .filter !="")
  counts(x) => (0..4).map (i -> x.filter (s -> s.substring i i+1 = "#") .count-1)
  locks := input.filter (x -> x[0].starts_with "#") .map counts
  keys  := input.filter (x -> x[0].starts_with ".") .map counts

  fits(l,k) => l.zip k (a,b -> a+b<=5) âˆ€ id

  part1 => locks.combine keys fits
                .count id

  part2 => "Merry Christmas!"

  say "$part1:$part2"
